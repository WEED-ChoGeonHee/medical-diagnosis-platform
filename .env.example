# 환경 변수 설정
# .env 파일을 복사하여 .env.local을 만들고 실제 값을 입력하세요

# 데이터베이스 설정
MONGODB_URI=mongodb://localhost:27017/medical-diagnosis
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=medical_diagnosis
MYSQL_USER=medical_user
MYSQL_PASSWORD=medical2024!

# JWT 시크릿 (프로덕션에서는 반드시 변경하세요)
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# OpenAI API 키 (실제 키로 변경하세요)
OPENAI_API_KEY=your-openai-api-key-here

# 서버 설정
PORT=5000
NODE_ENV=development
